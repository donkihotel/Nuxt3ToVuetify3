    "@vitejs/plugin-vue": "4.1.0",
    "eslint": "8.36.0",
    "eslint-config-prettier": "8.8.0",
    "jsdom": "21.1.1",
    "npm-run-all": "4.1.5",
    "nuxt": "^3.17.7",
    "prettier": "2.8.7",
    "sass": "1.60.0",
    "typescript": "5.0.2",
    "vue-eslint-parser": "9.1.0"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
